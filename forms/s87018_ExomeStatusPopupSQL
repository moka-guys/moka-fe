SELECT dbo_Patient_Table.PatientTrustID, dbo_Patient_Table.FirstName & " " & dbo_Patient_Table.LastName AS PatientName, Checker.UserName, Checker.Check1ID, Checker.Name AS RefName, NGSTest.DateRequested, Status.Status, dbo_Patient_Table.DoB, NGSTest.NGSPanelID, NGSTest.NGSPanelID_B, Referral.Referral, gw_GenderTable.Gender, NGSTest.NGSTestID, Status.StatusID, Patients.InternalPatientID, NGSPanel.PanelCode AS PanelCodeA, NGSPanel_1.PanelCode AS PanelCodeB, NGSTest.ResultComment, NGSWESBatch.BatchName, PedigreeItem.Item AS Relationship, AffectedStatusItem.Item AS Affected
FROM ((((((((((NGSTest INNER JOIN NGSPanel ON NGSTest.NGSPanelID = NGSPanel.NGSPanelID) INNER JOIN Patients ON NGSTest.InternalPatientID = Patients.InternalPatientID) INNER JOIN Referral ON NGSTest.ReferralID = Referral.ReferralID) INNER JOIN Checker ON NGSTest.BookBy = Checker.Check1ID) INNER JOIN Status ON NGSTest.StatusID = Status.StatusID) INNER JOIN (gw_GenderTable RIGHT JOIN dbo_Patient_Table ON gw_GenderTable.GenderID = dbo_Patient_Table.GenderID) ON Patients.PatientID = dbo_Patient_Table.PatientTrustID) LEFT JOIN NGSPanel AS NGSPanel_1 ON NGSTest.NGSPanelID_B = NGSPanel_1.NGSPanelID) LEFT JOIN NGSWESBatch ON NGSTest.WESBatch = NGSWESBatch.NGSWESBatchID) LEFT JOIN NGSAnalysis ON NGSTest.NGSTestID = NGSAnalysis.NGSTestID) LEFT JOIN Item AS PedigreeItem ON NGSAnalysis.Pedigree = PedigreeItem.ItemID) LEFT JOIN Item AS AffectedStatusItem ON NGSAnalysis.AffectedStatus = AffectedStatusItem.ItemID;
