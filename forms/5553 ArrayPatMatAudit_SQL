SELECT QFPCRRequest.RequestedDate, QFPCRRequest.ReferralID, QFPCRRequest.InternalPatientID, QFPCRRequest.RequestedByID, QFPCRRequest.BookedByID, QFPCRRequest.BookingCheckedByID, QFPCRRequest.StatusID, QFPCRRequest.Result, QFPCRRequest.ResultDate, QFPCRRequest.Comment, QFPCRRequest.AnalysedByID, QFPCRRequest.CheckedByID, QFPCRRequest.QFPCRRequestID, Patients.PatientID, [dbo_Patient_Table].[LastName] & " " & [dbo_Patient_Table].[Firstname] AS Name, Status_1.Status AS Rush, QFPCRRequest.AnalysedByDate, Patients.s_StatusOverall AS PatientStatus, Patients.Sexed, gw_SpecimenType.SpecimenTypeCode, dbo_Specimen_Table.SpecimenTrustID, Referral.ReferralID, Referral.Referral, QFPCRRequest.ResultDate FROM ((((QFPCRRequest INNER JOIN Patients ON QFPCRRequest.InternalPatientID = Patients.InternalPatientID) INNER JOIN Status AS Status_1 ON QFPCRRequest.Rush = Status_1.StatusID) LEFT JOIN (dbo_Specimen_Table LEFT JOIN gw_SpecimenType ON dbo_Specimen_Table.SpecimenTypeID = gw_SpecimenType.SpecimenTypeID) ON QFPCRRequest.SpecimenNo = dbo_Specimen_Table.SpecimenTrustID) INNER JOIN dbo_Patient_Table ON Patients.PatientID = dbo_Patient_Table.PatientTrustID) INNER JOIN Referral ON QFPCRRequest.ReferralID = Referral.ReferralID WHERE (((QFPCRRequest.StatusID)=4) AND ((Referral.ReferralID)=1199901152) AND ((QFPCRRequest.ResultDate)>=DateAdd("m",-12,Date()))) ORDER BY QFPCRRequest.RequestedDate DESC;
