SELECT DISTINCT Chromosome.Chr, PrimerAmplicon.Start19, PrimerAmplicon.Stop19, PrimerAmplicon.ForwardSeq, PrimerAmplicon.ReverseSeq, Status.Status, PrimerAmplicon.Notes, PrimerAmplicon.PrimerName, PrimerAmplicon.AmpliconID FROM (PrimerAmplicon INNER JOIN Status ON PrimerAmplicon.Status = Status.StatusID) INNER JOIN Chromosome ON PrimerAmplicon.ChromosomeID = Chromosome.ChrID 
