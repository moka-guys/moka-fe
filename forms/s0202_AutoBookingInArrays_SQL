SELECT Patients.PatientID, Patients.BookinLastName, Patients.BookinFirstName, ArrayTest.ArrayTestID, Patients.BookinDOB, Referral.Referral
FROM (ArrayTest INNER JOIN Patients ON ArrayTest.InternalPatientID = Patients.InternalPatientID) INNER JOIN Referral ON ArrayTest.ReferralID = Referral.ReferralID
WHERE (((ArrayTest.ReferralID)=1199901236));

