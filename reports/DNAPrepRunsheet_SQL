SELECT DNA.DNANumber, [BookinLastName] & " " & [BookinFirstName] AS Name, DNA.GelQualityID, DNA.Concentration, DNA.DNACheckRun, DNA.DNAComment, Patients.PatientID, DNA.Active, [RunNumber] AS Run, Item.Item AS TissueType, "Source: " & [Item_1].[Item] & " / From:" & [DNA_1].[DNANumber] & "(" & [DNA_1].[Concentration] & ")" AS [From], Patients.s_StatusOverall, DNA.ExtractionRun
FROM (((Patients INNER JOIN DNA ON Patients.InternalPatientID = DNA.InternalPatientID) LEFT JOIN Item ON DNA.TissueType = Item.ItemID) LEFT JOIN DNA AS DNA_1 ON DNA.FromDNAID = DNA_1.DNAID) LEFT JOIN Item AS Item_1 ON DNA.Source = Item_1.ItemID
WHERE (((DNA.DNANumber) Not Like "*C*" And (DNA.DNANumber) Not Like "*T*") AND ((DNA.GelQualityID) Is Null) AND ((DNA.Active)=True) AND ((Patients.s_StatusOverall)<>1202218798)) OR (((DNA.DNANumber) Not Like "*C*" And (DNA.DNANumber) Not Like "*T*") AND ((DNA.Concentration) Is Null) AND ((DNA.Active)=True) AND ((Patients.s_StatusOverall)<>1202218798)) OR (((DNA.DNANumber) Not Like "*C*" And (DNA.DNANumber) Not Like "*T*") AND ((DNA.DNACheckRun) Is Null) AND ((DNA.Active)=True) AND ((Patients.s_StatusOverall)<>1202218798)) OR (((DNA.DNANumber) Not Like "*C*" And (DNA.DNANumber) Like "*T*") AND ((DNA.GelQualityID) Is Null) AND ((DNA.Active)=True) AND ((Patients.s_StatusOverall)<>1168443116 And (Patients.s_StatusOverall)<>4 And (Patients.s_StatusOverall)<>1202218798) AND ((DNA.ExtractionRun) Is Not Null)) OR (((DNA.DNANumber) Not Like "*C*" And (DNA.DNANumber) Like "*T*") AND ((DNA.Concentration) Is Null) AND ((DNA.Active)=True) AND ((Patients.s_StatusOverall)<>1168443116 And (Patients.s_StatusOverall)<>4 And (Patients.s_StatusOverall)<>1202218798) AND ((DNA.ExtractionRun) Is Not Null)) OR (((DNA.DNANumber) Not Like "*C*" And (DNA.DNANumber) Like "*T*") AND ((DNA.DNACheckRun) Is Null) AND ((DNA.Active)=True) AND ((Patients.s_StatusOverall)<>1168443116 And (Patients.s_StatusOverall)<>4 And (Patients.s_StatusOverall)<>1202218798) AND ((DNA.ExtractionRun) Is Not Null))
ORDER BY DNA.DNANumber;
