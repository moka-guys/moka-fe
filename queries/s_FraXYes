SELECT s_FraXDiagnosticTests.RequestID, s_FraXDiagnosticTests.Disorder, s_FraXDiagnosticTests.TestClass, s_FraXDiagnosticTests.SpecimenNo, s_FraXDiagnosticTests.DateReceived, s_FraXDiagnosticTests.DNANo, s_FraXDiagnosticTests.Concentration, s_FraXDiagnosticTests.Name, s_FraXDiagnosticTests.Sex, Patients.OverallResultComments, s_FraXDiagnosticTests.DateRequested 
FROM (s_AbnormalResultPatientsNot INNER JOIN (Patients INNER JOIN dbo_dnaspecimenlinked ON Patients.PatientID = Str(dbo_dnaspecimenlinked.PatientID)) ON s_AbnormalResultPatientsNot.InternalPatientID = Patients.InternalPatientID) INNER JOIN s_FraXDiagnosticTests ON Str(dbo_dnaspecimenlinked.SpecimenID) = s_FraXDiagnosticTests.SpecimenNo;
