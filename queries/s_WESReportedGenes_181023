SELECT DISTINCT GenesHGNC_current.HGNCID, GenesHGNC_current.ApprovedSymbol AS Gene, Checker.Initials AS [Validated By], Format([Check4Date],"Short Date") AS [Date]
FROM ((((NGSVariantAnnotations INNER JOIN NGSVariant ON NGSVariantAnnotations.NGSVariantID = NGSVariant.NGSVariantID) INNER JOIN NGSTest ON NGSVariant.NGSTestID = NGSTest.NGSTestID) INNER JOIN Referral ON NGSTest.ReferralID = Referral.ReferralID) INNER JOIN GenesHGNC_current ON NGSVariantAnnotations.HGNCID = GenesHGNC_current.HGNCID) INNER JOIN Checker ON NGSTest.Check4ID = Checker.Check1ID
WHERE (((NGSVariantAnnotations.Reported)=-1) AND ((Referral.GroupTypeID)=14) AND ((NGSVariant.Classification) Is Not Null) AND ((NGSVariant.DateAdded)>#1/11/2018#)) AND GenesHGNC_current.HGNCID not in ('HGNC:132','HGNC:9722','HGNC:18040','HGNC:20730','HGNC:882','HGNC:14255','HGNC:1390','HGNC:1529','HGNC:13733','HGNC:17272','HGNC:20311','HGNC:20626','HGNC:1971','HGNC:2020','HGNC:2187','HGNC:2197','HGNC:2198','HGNC:2200','HGNC:2204','HGNC:2206','HGNC:2217','HGNC:2227','HGNC:2330','HGNC:14630','HGNC:2514','HGNC:2902','HGNC:2908','HGNC:2978','HGNC:3012','HGNC:29043','HGNC:2895','HGNC:30858','HGNC:29331','HGNC:3395','HGNC:21296','HGNC:3512','HGNC:3513','HGNC:3518','HGNC:3579','HGNC:3700','HGNC:3702','HGNC:3775','HGNC:4389','HGNC:5956','HGNC:17582','HGNC:6296','HGNC:18039','HGNC:19960','HGNC:7132','HGNC:7133','HGNC:6814','HGNC:23212','HGNC:7573','HGNC:7576','HGNC:7622','HGNC:7720','HGNC:5331','HGNC:7881','HGNC:7907','HGNC:7983','HGNC:8515','HGNC:14270','HGNC:15673','HGNC:8940','HGNC:26270','HGNC:14938','HGNC:9009','HGNC:9115','HGNC:18801','HGNC:9179','HGNC:17264','HGNC:9302','HGNC:9588','HGNC:9811','HGNC:21406','HGNC:9957','HGNC:29168','HGNC:10483','HGNC:10484','HGNC:21637','HGNC:10596','HGNC:14295','HGNC:10848','HGNC:15454','HGNC:13818','HGNC:10994','HGNC:20653','HGNC:6768','HGNC:11098','HGNC:11100','HGNC:11103','HGNC:11654','HGNC:11720','HGNC:11772','HGNC:12303','HGNC:12340','HGNC:18083','HGNC:12363','HGNC:20774','HGNC:12442','HGNC:25792','HGNC:20347','HGNC:13829','HGNC:13030','HGNC:14881','HGNC:16700','HGNC:16966')
ORDER BY GenesHGNC_current.ApprovedSymbol;
