CREATE TABLE [dbo].[Status](
	[StatusID] [int] IDENTITY(1,1) NOT NULL,
	[Status_old] [nvarchar](50) NOT NULL,
	[Array] [bit] NOT NULL,
	[SortingArray] [int] NULL,
	[FollowUpTest] [bit] NOT NULL,
	[SortingFollowUpTest] [int] NULL,
	[Overall] [bit] NOT NULL,
	[SortingOverall] [int] NULL,
	[Test] [bit] NOT NULL,
	[SortingTest] [int] NULL,
	[MLPA] [bit] NOT NULL,
	[SortingMLPA] [int] NULL,
	[YesNo] [bit] NOT NULL,
	[QFPCR] [int] NULL,
	[SortingQFPCR] [int] NULL,
	[Pathogenic] [int] NULL,
	[SortingPathogenic] [int] NULL,
	[FraXRequest] [int] NULL,
	[SortingFraXRequest] [int] NULL,
	[NGS] [int] NULL,
	[SortingNGS] [int] NULL,
	[PrimerDB] [tinyint] NULL,
	[SortedPrimerDB] [tinyint] NULL,
	[Reporting] [tinyint] NULL,
	[SortingReporting] [tinyint] NULL,
	[Status] [nvarchar](70) NULL,
	[GMSstatus] [tinyint] NULL,
	[SortingGMSstatus] [tinyint] NULL,
 CONSTRAINT [PK_Status] PRIMARY KEY CLUSTERED 
(
	[StatusID] ASC
)WITH (PAD_INDEX = OFF, STATISTICS_NORECOMPUTE = OFF, IGNORE_DUP_KEY = OFF, ALLOW_ROW_LOCKS = ON, ALLOW_PAGE_LOCKS = ON) ON [PRIMARY]
) ON [PRIMARY]

GO

